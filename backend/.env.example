# Application Configuration
NODE_ENV=development
PORT=3001

# Database (Get from Aiven or your provider)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public&sslmode=require" # Adjust sslmode as needed

# Security
JWT_SECRET="YOUR_SUPER_SECRET_KEY_CHANGE_ME" # Use a strong, random string
JWT_EXPIRES_IN="7d"
BCRYPT_SALT_ROUNDS=10

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173